{
  "name": "eslint-next-error",
  "displayName": "ESLint Next Error",
  "description": "Jump to next ESLint error in workspace",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rsml/eslint-next-error"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:eslint-next-error.jumpToNextError"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "eslint-next-error.jumpToNextError",
        "title": "Jump to Next ESLint Error in Workspace"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}